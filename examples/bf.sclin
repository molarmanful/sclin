  "+" ( =$e $e 1.+ [ "m" $e"p": /]% ) ,.
  "-" ( =$e $e 1.- [ "m" $e"p": /]% ) ,.
  "." ( =$e $e ([ "m" $e"p": /] ) Q c>S >o ) ,.
