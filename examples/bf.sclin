]:

#brack =$e.
  $e [ "m" $e"p": /] \; 2.@~ ?#
    $e ( $e"i": ).' "s":%
    0 $e ; nip
    ; \@ &#
      1.+ "i":%.
      $code over"i": : [.
        \!    ( pop 0 ) ,.
        "[".= ( pop 1.+ dip 1 ) ,.
        "]".= ( pop over ( 1.- dip 1 ) 0 ?# ) ,.
      ]: ??#

